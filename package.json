  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlee-dev/navvy.git"
  },
  "keywords": [
    "navbar",
    "react"
  ],
  "author": "Alex Lee <alex@alexlee.site>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexlee-dev/navvy/issues"
  },
  "homepage": "https://github.com/alexlee-dev/navvy#readme",
  "dependencies": {
    "grommet": "^2.7.1",
    "grommet-icons": "^4.2.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@testing-library/react": "^8.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "files": [
    "dist/main.js",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
